	text-align:center;
	display: inline;
    font-family:Tahoma;
	font-size:13px;
	color:#FFFFFF;
	font-weight:bold;
	text-decoration:none;
	/*border:#FF0000 solid 1px;*/
	
}
.bodyContainer  .rightPanel .boxes ul li span a.link{
	color: #FFFFFF;
    display: inline; 
    float: left;
    font-family: Tahoma;
    font-size: 13px;
    font-weight: bold;
    margin-left: 3px;
    margin-top: 5px;
    text-align: center;
    text-decoration: none;
	width:100%;
}
