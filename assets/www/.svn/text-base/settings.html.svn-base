	function saveLogOffScheduele(){
		if(document.getElementById("LogOffSchedule").checked == true){
			
			var startTime = document.getElementById("startTime").value;
			var endTime = document.getElementById("endTime").value;
			cordova.exec(saveLogoffSuccess, saveLogoffError, "Contact", "saveLogoffSchedule", [startTime , endTime]);
					
		}
	}

	function saveLogoffSuccess(){
		console.error("saveLogoffSuccess")
	}
	
	function saveLogoffError(){
		console.error("saveLogoffError")
	}

